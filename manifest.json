{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "content_scripts": [
    {
      "js": ["tinycolor.js", "main.js"],
      "matches": [
        "https://calendar.google.com/*",
        "https://www.calendar.google.com/*"
      ]
    }
